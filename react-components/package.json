{
    "name": "react-components",
    "version": "0.1.0",
    "homepage": "/de/react_components",
    "private": true,
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.3.0",
        "@babel/plugin-proposal-decorators": "^7.3.0",
        "@storybook/addon-actions": "^5.2.5",
        "@storybook/addon-console": "^1.2.1",
        "@storybook/addon-knobs": "^5.2.5",
        "@storybook/addon-links": "^5.2.5",
        "@storybook/addons": "^5.2.5",
        "@storybook/react": "^5.2.5",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "husky": "^1.3.1",
        "jest-junit-reporter": "^1.1.0",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "prop-types": "^15.7.2"
    },
    "dependencies": {
        "@cyverse-de/ui-lib": "^0.3.17",
        "@date-io/date-fns": "^1.3.13",
        "@material-ui/core": "^4.5.2",
        "@material-ui/icons": "^4.5.1",
        "@material-ui/lab": "^4.0.0-alpha.31",
        "@material-ui/pickers": "^3.2.8",
        "add": "^2.0.6",
        "classnames": "^2.2.6",
        "date-fns": "^2.0.0",
        "formik": "^1.5.0",
        "intro.js": "^2.9.3",
        "numeral": "^2.0.6",
        "object-assign": "4.1.1",
        "promise": "8.0.1",
        "rc-tree": "^1.14.10",
        "react": "^16.8.6",
        "react-dom": "^16.8.6",
        "react-intl": "^2.8.0",
        "react-player": "^1.6.4",
        "react-scripts": "2.1.8",
        "react-select": "^2.0.0",
        "sanitize-html": "^2.3.2",
        "showdown": "^1.9.1",
        "sockette": "2.0.0",
        "sw-precache-webpack-plugin": "0.11.5",
        "whatwg-fetch": "2.0.3"
    },
    "scripts": {
        "start": "npm run storybook",
        "build": "REACT_APP_MODE=development node scripts/build.js",
        "prod": "REACT_APP_MODE=production node scripts/build.js",
        "test": "react-scripts test --env=jsdom",
        "test:ci": "CI=true react-scripts test --env=jsdom --ci --testResultsProcessor ./node_modules/jest-junit-reporter",
        "storybook": "start-storybook -p 6006",
        "build-storybook": "build-storybook",
        "format": "prettier --write 'src/**/*.js' 'stories/**/*.js'"
    },
    "babel": {
        "presets": [
            "react-app"
        ]
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "prettier": {
        "trailingComma": "es5",
        "tabWidth": 4,
        "bracketSpacing": true,
        "arrowParens": "always",
        "jsxBracketSameLine": false
    },
    "husky": {
        "hooks": {
            "pre-commit": "pretty-quick --staged"
        }
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ]
}
